begin
prog = irexec
button = stop
config = /usr/bin/mpc stop
end

begin
prog = irexec
button = up
config = /usr/bin/mpc seek +5
delay = 2
repeat = 3
end

begin
prog = irexec
button = down
config = /usr/bin/mpc seek -5
delay = 2
repeat = 3
end

begin
prog = irexec
button = next
config = /usr/bin/mpc next
end

begin
prog = irexec
button = prev
config = /usr/bin/mpc prev
end

begin
prog = irexec
button = pause
config = /usr/bin/mpc toggle
delay = 2
repeat = 3
end